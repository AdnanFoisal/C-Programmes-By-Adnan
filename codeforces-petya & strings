#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
	char a[100],b[100];
	
		scanf("%s %s",a,b);
		for(int i=0;a[i]!='\0';i++)
		{
			a[i]=tolower(a[i]);
			}
		for(int i=0;b[i]!='\0';i++)
		{
			b[i]=tolower(b[i]);
			}
		if(strcmp(a,b)<0)
		printf("-1");
	else	if(strcmp(a,b)>0)
		printf("1");
	else
		printf("0");
			
		

	}