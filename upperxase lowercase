#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
	int i,countu=0,countl=0;
	char a[100];
	scanf("%s",a);
	size_t len= strlen(a);
	for(i=0;i<len;i++)
	{
		if(isupper(a[i]))
		{
			countu++;
			}
			else
			countl++;
		}
		for(i=0;i<len;i++)
		{
		if(countu>countl)
		{
			a[i]=toupper(a[i]);
		
		}
		
		
	else
		{
			a[i]=tolower(a[i]);
	
		}

	}
	printf("%s",a);
	}