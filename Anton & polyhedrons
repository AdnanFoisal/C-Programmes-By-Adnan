#include<stdio.h>
#include<string.h>
int main()
{
	int n,i,sum=0;
	scanf("%d",&n);
	char c[15];
	for(i=0;i<n;i++)
	{
		scanf("%s",c);
		if (strcmp(c, "Tetrahedron") == 0) {
            sum += 4;
        } else if (strcmp(c, "Cube") == 0) {
            sum += 6;
        } else if (strcmp(c, "Octahedron") == 0) {
            sum += 8;
        } else if (strcmp(c, "Dodecahedron") == 0) {
            sum += 12;
        } else if (strcmp(c, "Icosahedron") == 0) {
            sum += 20;
        }
	
		}
	
		printf("%d",sum);
	}