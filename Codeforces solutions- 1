#include<stdio.h>
int main()
{
	int t,ans,n,i;
	scanf("%d",&t);
	while(t--)
	{
		ans=0;
		scanf("%d",&n);
		int a[n];
		int c0=0,c1=0,c2=0,c3=0,c5=0;
	
		for(i=0;i<n;i++)
		{
			
			scanf("%d",&a[i]);
			int val=a[i]%10;
			if(val==0)
			c0++;
			else if(val==1)
			c1++;
			else if(val==2)
			c2++;
			else if(val==3)
			c3++;
			else if(val==5)
			c5++;
			if(ans==0 && c0>=3 && c1>=1 && c2>=2 && c3>=1 && c5>=1)
				ans=i+1;
				}
				
				
				
				printf("%d\n",ans);
			}
			
		}