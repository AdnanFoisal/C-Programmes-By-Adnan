#include<stdio.h>
int main()
{
	int i,h[300],a[300],j,n,count=0;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d %d",&h[i],&a[i]);
		}
		h[n]='\0';
		a[n]='\0';
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if (i!=j)
			{
				if(h[i]==a[j])
				{
					count++;
					
					}
				}
			}
		}
		printf("%d",count);
		return 0;
	}